{
    "name": "vscode-slither",
    "displayName": "vscode-slither",
    "description": "Slither VS code extension",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.slither"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.slither",
                "title": "Run Slither"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "@types/shelljs": "^0.8.1",
        "tslint": "^5.8.0",
        "typescript": "^3.1.4",
        "vscode": "^1.1.25"
    },
    "dependencies": {
        "chalk": "^2.4.1",
        "shelljs": "^0.8.3"
    }
}
